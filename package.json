{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "live": "nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps",
    "open": "npm run open:local",
    "open:remote": "node server.js -o https://cv-generator-life-map.herokuapp.com/",
    "open:local": "ng serve --open=true"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branches": [
      "master"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/platform-server": "^14.1.0",
    "@angular/pwa": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@snyk/protect": "^1.981.0",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "angular-plotly.js": "4.0.4",
    "chart.js": "^3.8.2",
    "compression": "1.7.4",
    "express": "4.18.1",
    "newrelic": "^8.16.0",
    "npm-run-all": "4.1.5",
    "rxjs": "^7.5.6",
    "spdy": "^4.0.2",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-devkit/core": "^14.1.0",
    "@angular-devkit/schematics": "^14.1.0",
    "@angular-eslint/builder": "^14.0.2",
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular-eslint/eslint-plugin-template": "^14.0.2",
    "@angular-eslint/schematics": "^14.0.2",
    "@angular-eslint/template-parser": "^14.0.2",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@compodoc/compodoc": "1.1.19",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@szmarczak/http-timer": "5.0.1",
    "@types/chart.js": "2.9.37",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "auto-changelog": "2.4.0",
    "codecov": "3.8.3",
    "conventional-changelog-angular": "^5.0.13",
    "coveralls": "3.1.1",
    "enhanced-resolve": "5.10.0",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "figlet": "^1.5.2",
    "figlet-cli": "^0.1.1",
    "jasmine-core": "4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lighthouse": "^9.6.3",
    "protractor": "7.0.0",
    "sass": "^1.54.0",
    "semantic-release": "^19.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.7"
  },
  "engines": {
    "node": "^14.15.0 || >=16.10.0",
    "npm": "^6.11.0 || ^7.5.6 || >=8.0.0",
    "yarn": ">= 1.13.0"
  },
  "snyk": true
}
