{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "live": "nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps",
    "open": "npm run open:local",
    "open:remote": "node server.js -o https://cv-generator-life-map.herokuapp.com/",
    "open:local": "ng serve --open=true"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branches": [
      "master"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "dependencies": {
    "0": "^0.0.0",
    "@angular/animations": "^17.0.2",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/platform-server": "^17.0.2",
    "@angular/pwa": "^17.0.0",
    "@angular/router": "^17.0.2",
    "@angular/service-worker": "^17.0.2",
    "@snyk/protect": "^1.1245.0",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "angular-plotly.js": "5.2.2",
    "chart.js": "^4.4.0",
    "compression": "1.7.4",
    "express": "4.18.2",
    "heroku": "~8.7.1",
    "newrelic": "^11.5.0",
    "npm-run-all": "4.1.5",
    "rxjs": "^7.8.1",
    "spdy": "^4.0.2",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-devkit/core": "^17.0.0",
    "@angular-devkit/schematics": "^17.0.0",
    "@angular-eslint/builder": "^17.0.1",
    "@angular-eslint/eslint-plugin": "^17.0.1",
    "@angular-eslint/eslint-plugin-template": "^17.0.1",
    "@angular-eslint/schematics": "^17.0.1",
    "@angular-eslint/template-parser": "^17.0.1",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@angular/language-service": "^17.0.2",
    "@compodoc/compodoc": "1.1.22",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@szmarczak/http-timer": "5.0.1",
    "@types/chart.js": "2.9.40",
    "@types/jasmine": "5.1.2",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "20.9.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "auto-changelog": "2.4.0",
    "codecov": "3.8.3",
    "conventional-changelog-angular": "^7.0.0",
    "coveralls": "3.1.1",
    "enhanced-resolve": "5.15.0",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "figlet": "^1.7.0",
    "figlet-cli": "^0.1.1",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lighthouse": "^11.3.0",
    "protractor": "7.0.0",
    "sass": "^1.69.5",
    "semantic-release": "^22.0.7",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "^16.13.0 || >=18.10.0 <19",
    "npm": "^6.11.0 || ^7.5.6 || >=8.0.0 <10",
    "yarn": ">= 1.13.0"
  },
  "snyk": true
}
