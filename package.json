{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "live": "nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps",
    "open": "npm run open:local",
    "open:remote": "node server.js -o https://cv-generator-life-map.herokuapp.com/",
    "open:local": "ng serve --open=true"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branches": [
      "master"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "overrides": {
    "@opentelemetry/api": "1.9.0",
    "nopt": "^9",
    "@npmcli/installed-package-contents": "^4",
    "rimraf": "^6",
    "uuid": "^9",
    "glob": "^10",
    "boom": "@hapi/boom",
    "cryptiles": "@hapi/cryptiles",
    "sntp": "@hapi/sntp",
    "hoek": "@hapi/hoek",
    "hawk": "@hapi/hawk",
    "inflight": "1.0.6",
    "request": "^2.88.2",
    "oclif": "^4"
  },
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/platform-server": "^21.1.2",
    "@angular/pwa": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@angular/service-worker": "^21.1.2",
    "@snyk/protect": "^1.1302.1",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "angular-plotly.js": "20.0.0",
    "bootstrap": "^4.6.2",
    "compression": "1.8.1",
    "express": "5.2.1",
    "heroku": "~10.16.0",
    "jquery": "^3.7.1",
    "newrelic": "^13.12.0",
    "npm-run-all": "4.1.5",
    "rxjs": "^7.8.2",
    "spdy": "^4.0.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.2",
    "@angular-devkit/core": "^21.1.2",
    "@angular-devkit/schematics": "^21.1.2",
    "@angular-eslint/builder": "^21.2.0",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/schematics": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@angular/language-service": "^21.1.2",
    "@compodoc/compodoc": "1.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@szmarczak/http-timer": "5.0.1",
    "@types/jasmine": "6.0.0",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "25.2.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "auto-changelog": "2.5.0",
    "codecov": "3.8.3",
    "conventional-changelog-angular": "^8.1.0",
    "coveralls": "3.1.1",
    "enhanced-resolve": "5.19.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "62.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "figlet": "^1.10.0",
    "figlet-cli": "^0.3.0",
    "jasmine-core": "^5.13.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "2.2.0",
    "lighthouse": "^13.0.1",
    "protractor": "7.0.0",
    "sass": "^1.97.3",
    "semantic-release": "^25.0.3",
    "ts-node": "10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0 <25",
    "npm": ">=10.0.0 <12"
  },
  "snyk": true
}
