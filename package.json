{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "live": "nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps",
    "open": "npm run open:local",
    "open:remote": "node server.js -o https://cv-generator-life-map.herokuapp.com/",
    "open:local": "ng serve --open=true"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      {
        "npmPublish": false
      },
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "dependencies": {
    "@angular/animations": "^12.2.5",
    "@angular/common": "^12.2.5",
    "@angular/compiler": "^12.2.5",
    "@angular/core": "^12.2.5",
    "@angular/forms": "^12.2.5",
    "@angular/platform-browser": "^12.2.5",
    "@angular/platform-browser-dynamic": "^12.2.5",
    "@angular/platform-server": "^12.2.5",
    "@angular/pwa": "^12.2.5",
    "@angular/router": "^12.2.5",
    "@angular/service-worker": "^12.2.5",
    "@oclif/command": "^1.8.0",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "ajv": "^8.6.3",
    "angular-plotly.js": "4.0.4",
    "chart.js": "3.5.1",
    "compression": "1.7.4",
    "express": "4.17.1",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "newrelic": "^8.3.0",
    "nock": "^13.1.3",
    "npm-run-all": "4.1.5",
    "rxjs": "^7.3.0",
    "snyk": "^1.707.0",
    "spdy": "^4.0.2",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.5",
    "@angular-eslint/builder": "12.4.1",
    "@angular-eslint/eslint-plugin": "12.4.1",
    "@angular-eslint/eslint-plugin-template": "12.4.1",
    "@angular-eslint/schematics": "12.4.1",
    "@angular-eslint/template-parser": "12.4.1",
    "@angular/cli": "^12.2.5",
    "@angular/compiler-cli": "^12.2.5",
    "@angular/language-service": "^12.2.5",
    "@compodoc/compodoc": "1.1.14",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@szmarczak/http-timer": "5.0.1",
    "@types/chart.js": "2.9.34",
    "@types/jasmine": "3.9.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "auto-changelog": "2.3.0",
    "codecov": "3.8.3",
    "conventional-changelog-angular": "^5.0.13",
    "coveralls": "3.1.1",
    "enhanced-resolve": "5.8.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "figlet": "^1.5.2",
    "figlet-cli": "^0.1.1",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lighthouse": "8.4.0",
    "protractor": "7.0.0",
    "sass": "^1.39.2",
    "semantic-release": "^17.4.7",
    "ts-node": "10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "snyk": true
}
