{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "live": "nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps",
    "open": "npm run open:local",
    "open:remote": "node server.js -o https://cv-generator-life-map.herokuapp.com/",
    "open:local": "ng serve --open=true"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      {
        "npmPublish": false
      },
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/platform-server": "^13.2.2",
    "@angular/pwa": "^13.2.3",
    "@angular/router": "^13.2.2",
    "@angular/service-worker": "^13.2.2",
    "@oclif/command": "^1.8.16",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "ajv": "^8.10.0",
    "angular-plotly.js": "4.0.4",
    "chart.js": "3.7.1",
    "compression": "1.7.4",
    "express": "4.17.2",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "newrelic": "^8.7.1",
    "nock": "^13.2.4",
    "npm-run-all": "4.1.5",
    "rxjs": "^7.5.4",
    "snyk": "^1.852.0",
    "spdy": "^4.0.2",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@compodoc/compodoc": "1.1.18",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@szmarczak/http-timer": "5.0.1",
    "@types/chart.js": "2.9.35",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "auto-changelog": "2.4.0",
    "codecov": "3.8.3",
    "conventional-changelog-angular": "^5.0.13",
    "coveralls": "3.1.1",
    "enhanced-resolve": "5.9.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "figlet": "^1.5.2",
    "figlet-cli": "^0.1.1",
    "jasmine-core": "4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lighthouse": "9.3.1",
    "protractor": "7.0.0",
    "sass": "^1.49.7",
    "semantic-release": "^19.0.2",
    "ts-node": "10.5.0",
    "typescript": "4.4"
  },
  "engines": {
    "node": "<17",
    "npm": "<8.2"
  },
  "snyk": true
}
