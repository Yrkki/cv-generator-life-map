{
  "name": "cv-generator-life-map",
  "version": "1.1.2",
  "description": "CV generator life map frontend",
  "keywords": [
    "CV generator"
  ],
  "author": "Georgi Marinov",
  "homepage": "https://yrkki.github.io/cv-generator-life-map/",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "live": "nodemon server.js",
    "build": "ng build",
    "buildprod": "ng build --prod",
    "buildfast": "ng build --stats-json --source-map=false",
    "heroku-postbuild": "npm run buildprod",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install",
    "update-packages:ng-update": "ng update",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates -u --packageFile package.json && npm install"
  },
  "main": "src/main.ts",
  "private": false,
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      {
        "npmPublish": false
      },
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yrkki/cv-generator-life-map.git"
  },
  "dependencies": {
    "@angular/animations": "^11.1.0-next.0",
    "@angular/common": "^11.1.0-next.0",
    "@angular/compiler": "11.1.0-next.0",
    "@angular/core": "^11.1.0-next.0",
    "@angular/forms": "^11.1.0-next.0",
    "@angular/platform-browser": "^11.1.0-next.0",
    "@angular/platform-browser-dynamic": "^11.1.0-next.0",
    "@angular/platform-server": "^11.1.0-next.0",
    "@angular/pwa": "^0.1100.2",
    "@angular/router": "^11.1.0-next.0",
    "@angular/service-worker": "^11.1.0-next.0",
    "@tailorbrands/node-exporter-prometheus": "2.0.7",
    "ajv": "^6.12.3",
    "angular-plotly.js": "3.0.0",
    "chart.js": "2.9.4",
    "compression": "1.7.4",
    "eslint": "^6.7.2",
    "express": "4.17.1",
    "heroku": "^7.47.3",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "newrelic": "^7.0.1",
    "nock": "^9.6.1",
    "npm-run-all": "4.1.5",
    "rxjs": "^6.6.3",
    "snyk": "^1.432.1",
    "spdy": "^4.0.2",
    "tslib": "^2.1.0",
    "tslint-angular": "3.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.2",
    "@angular/cli": "^11.1.0-next.0",
    "@angular/compiler-cli": "^11.1.0-next.0",
    "@angular/language-service": "11.1.0-next.0",
    "@compodoc/compodoc": "1.1.11",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.8",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@szmarczak/http-timer": "4.0.5",
    "@types/chart.js": "2.9.28",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.10",
    "auto-changelog": "2.2.1",
    "codecov": "3.8.1",
    "codelyzer": "^6.0.1",
    "conventional-changelog-angular": "^5.0.12",
    "coveralls": "3.1.0",
    "enhanced-resolve": "5.3.2",
    "figlet": "^1.5.0",
    "figlet-cli": "^0.1.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lighthouse": "6.5.0",
    "protractor": "7.0.0",
    "sass": "^1.29.0",
    "semantic-release": "^17.3.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "14.11.0",
    "npm": "6.14.8"
  },
  "snyk": true
}